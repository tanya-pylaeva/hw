pentagonal_numbers = set()
for i in range(1, 5000):
    for j in range(i + 1, 5000):
        number1 = j * (3 * j - 1) // 2
        number2 = i * (3 * i - 1) // 2
        subtraction = number1 - number2
        addition = number1 + number2
        if number1 not in pentagonal_numbers:                
            pentagonal_numbers.add(number1)
        if addition in pentagonal_numbers and subtraction in pentagonal_numbers and addition in pentagonal_numbers:
            print(subtraction)
